{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": "error",
        "camelcase": 0,
        "max-len": 0,
        "object-curly-spacing": 0,
        "comma-dangle": [
            "error",
            {
                "arrays": "always",
                "objects": "always"
            }
        ]
    }
}